{
  "expo": {
    "name": "Nightout",
    "slug": "nightout-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "nightout-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0b0f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture photos and videos for activities.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photos to select images for activities.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record videos for reels."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121214"
      },
      "package": "com.antum.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Nightout to access your photos.",
          "cameraPermission": "Allow Nightout to access your camera."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Nightout to access your camera.",
          "microphonePermission": "Allow Nightout to access your microphone.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Nightout to access your photos.",
          "savePhotosPermission": "Allow Nightout to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-secure-store",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "364be99e-1b1a-4ee0-b38c-46e3284be8b4"
      }
    },
    "owner": "hadwishes"
  }
}
