hadismainuk@Hadis-MacBook-Air-2 nightout-app % # go to backend
cd ~/Desktop/NightOut/nightout-app/backend

# kill anything using port 4001 (safe)
lsof -ti:4001 | xargs kill -9 2>/dev/null || true

# install deps (only needed once, but safe to run)
npm install

# prisma (safe to run; if you already migrated it will just be quick)
npx prisma generate
npx prisma migrate dev

# start backend on 4001
PORT=4001 npm run dev
